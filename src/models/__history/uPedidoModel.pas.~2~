type
  TPedidoItem = class
  public
    AutoInc: Integer;
    Numero: Integer;
    CodProduto: Integer;
    Quantidade: Double;
    ValorUnit: Double;
    ValorTotal: Double;
  end;

  TPedido = class
  public
    Numero: Integer;
    Data: TDate;
    CodCliente: Integer;
    ValorTotal: Double;
    Itens: TObjectList<TPedidoItem>;
    constructor Create;
    destructor Destroy; override;
  end;

